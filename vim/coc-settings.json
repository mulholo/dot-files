{
  "coc.source.dictionary.priority": 1,
  "coc.source.word.priority": 1,
  "coc.source.buffer.priority": 2,
  "coc.source.around.priority": 3,
  "coc.source.file.priority": 4,
  "coc.source.ultisnips.priority": 10,
  "coc.preferences.jumpCommand": "drop",
  "coc.preferences.hoverTarget": "float",
  "diagnostic.refreshOnInsertMode": true,
  "suggest.floatEnable": true,
  "suggest.noselect": false,
  "suggest.acceptSuggestionOnCommitCharacter": true,
  "suggest.enablePreview": true,
  "suggest.enablePreselect": true,
  "diagnostic.maxWindowHeight": 30,
  "signature.maxWindowHeight": 20,
  "diagnostic.checkCurrentLine": true,
  "coc.preferences.currentFunctionSymbolAutoUpdate": true,
  "coc.preferences.formatOnSaveFiletypes": [
    "css",
    "javascript",
    "javascriptreact",
    "typescript",
    "typescriptreact"
  ],
  "prettier.requireConfig": false,
  "tsserver.formatOnType": true,
  "jest.terminalPosition": "bottom",
  "snippets.userSnippetsDirectory": "~/.dot-files/vim/UltiSnips",
  "snippets.extends": {
    "javascriptreact": ["javascript"],
    "typescript": ["javascript"],
    "html": ["javascript"],
    "typescriptreact": ["javascriptreact"]
  },
  "coc.preferences.formatOnType": true
}
